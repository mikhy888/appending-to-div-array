	 $('.main-level li:first').addClass("main-level-first");

	 $('.main-level-first a').html(function(index,html){
        return html.replace(/®/g , "®");
   	 });
	 $('.main-level-first a').html(function(index,html){
        return html.replace(/™/g , "™");
   	 });

