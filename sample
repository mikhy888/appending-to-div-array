<script type="application/javascript" src="http://192.168.1.9/optifast-me_com/sites/all/themes/optifast/assets/js/slick.min.js"></script>
<script src="http://192.168.1.9/optifast-me_com/sites/all/themes/optifast/assets//js/readmore.min.js"></script>
<script type="text/javascript">
    /*slider initializations*/
    var mobtitleSlider = {
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: true,
                accesibility: false,
                draggable: false,
                fade: true,
                rtl: false,
                //asNavFor: '.ac-content-inner'
            }
    var mobcontentSlider = {
        draggable: false,
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: false,
        draggable: false,
        fade: true,
        asNavFor: '.mob-table-slider',
        rtl: false,
    }
    
    
   $(document).ready(function() {
    /*hiding unanter elements*/
    $('.ac-content').hide();
    $('.ac-content:first').addClass("opened-acc");
    $('.ac-button:first').addClass("on");
    $('.blue-strip-table').hide();
    $('.blue-strip-table:first').show();
    
    /*Accordion scripts*/
    $('.ac-button').click(function() {
        var arr = null;
        arr = new Array();
        $('.ac-content').slideUp('normal').removeClass('opened-acc');
        $('.ac-content-inner').removeClass("added-tb-con");
        $('.added-tb-con ul').removeClass('added-c-ul');
        $('.ac-button').removeClass('on');
        
        /*generating the slider contents for mobile slider*/
        if($(this).next().is(':hidden') == true) {
            $(this).next().slideDown('normal').addClass("opened-acc");
            $(this).addClass('on');
            $('.on').next('.ac-content').children('.mob-table-slider-container').children('.ac-content-inner').addClass("added-tb-con");
            var phrases = [];
            var phrase = '';
            $(".added-tb-con ul").find('li:first').each(function(){
                phrase += "<div>"+$(this).text()+"</div>";
            });
            phrases.push(phrase);
            $(".mob-table-slider").html(phrases);
         }
        
        /*accordion initialization*/
        var gin = $(this).closest('.ac-main').find('.ac-button').index(this);
        $('.ac-main').find('.blue-strip-table:first').siblings().hide();
        $('.ac-main').find('.blue-strip-table:nth-child('+(gin+1)+')').fadeIn(300).siblings().hide();
        
        /*slider initialization*/
        if( $(window).width() < 768 ) {     
            $('.mob-table-slider').removeClass("slick-initialized slick-slider");
            setTimeout(function(){
                $(".added-tb-con .content-table").hide();
                $(".added-tb-con .content-table:first").show();
                $('.opened-acc .mob-table-slider').not('.slick-initialized').slick(mobtitleSlider);
            }, 200);
        }
    }); /*click ends here*/
       
    /*table blus strip movement*/
    if( $(window).width() > 768 ) {
        $('.ac-content:first').slideDown();
        $( ".blue-strip-table" ).appendTo(".desktop-thead");
    } else if($(window).width() < 768) {
        //$('.blue-strip-table').show();
        /*appending the first li item for dropdown*/
        var getfirstlidp = $('.mpdp li:first').html();
        $('.mpdp').prepend('<li class="init">'+getfirstlidp+'</li>');
        
        /*blue strip appending*/
        //$( ".blue-strip-table" ).prependTo(".blue-strip");
        /*slick slider initialization for slides*/
        setTimeout(function(){
            $(".ac-button:first").trigger("click");
            $('.opened-acc .mob-table-slider').not('.slick-initialized').slick(mobtitleSlider);
        }, 200);
        
        /*getting index to show the paricular table item*/
        $(".added-tb-con .content-table").hide();
         $('.mob-table-slider').on('afterChange', function(){
            var currentmobindex = $('.opened-acc .mob-table-slider').slick('slickCurrentSlide');
            $(".added-tb-con .content-table").hide();
            $('.added-tb-con .content-table:eq(' + currentmobindex + ')').fadeIn();
         });
        /*hiding the accordion elements*/
        $(".ac-button").hide();
        
        
        
        $(".mpdp").on("click", ".init", function() {
            $(this).closest(".mpdp").children('li:not(.init)').toggle();
        });

        var allOptions = $(".mpdp").children('li:not(.init)');
        $(".mpdp").on("click", "li:not(.init)", function() {
                
            /*default sropdown scripts*/
            allOptions.removeClass('selected');
            $(this).addClass('selected');
            $(".mpdp").children('.init').html($(this).html());
            allOptions.hide();
            
            /*code for showing tables*/
            var arr = null;
            arr = new Array();  
            $('.ac-content-inner').removeClass("added-tb-con");
            $('.added-tb-con ul').removeClass('added-c-ul');
            $('.ac-button').removeClass('on');

            $(".ac-content").removeClass("opened-acc").hide();
            $(".blue-strip .blue-strip-table").hide();
            var smdpv = $(this).attr("data-value");
            var smdpvnu = $(this).attr("data-did");
            //$(".ac-content").siblings('[data-content=' + smdpv + ']').addClass('opened-acc').show();
            //$('[data-content=' + smdpv + ']').children('.mob-table-slider-container').children('.ac-content-inner').addClass("added-tb-con");
            
            $("#ac-content" + smdpvnu).addClass('opened-acc').show();
            $("#ac-content-inner" + smdpvnu).addClass("added-tb-con");
            $("#ac-content" + smdpvnu).find("#ac-content-ul"  + smdpvnu).show();
            //$(".ac-content.opened-acc").find(".blue-strip-table[data-content=" + smdpv + "]").show();
            
            var phrases = [];
                var phrase = '';
                $(".added-tb-con ul").find('li:first').each(function(){
                    phrase += "<div>"+$(this).text()+"</div>";
                });
                phrases.push(phrase);
                $(".mob-table-slider").html(phrases);

             $('.mob-table-slider').removeClass("slick-initialized slick-slider");
                setTimeout(function(){
                    $(".added-tb-con .content-table").hide();
                    $(".added-tb-con .content-table:first").show();
                    $('.opened-acc .mob-table-slider').not('.slick-initialized').slick(mobtitleSlider);
                }, 200);    

            //$(".blue-strip .blue-strip-table").hide();
            //$('.blue-strip .blue-strip-table[data-content=' + smdpv + ']').show();
            
        }); /*dropdown event call close here*/
    }
       
    
    /*resizing window*/
    var resizeTimer = null;  
    $(window).resize(function(){
        clearTimeout(resizeTimer);
        resizeTimer= setTimeout(function(){ 
          if( $(window).width() > 768 ) {
                $( ".blue-strip-table" ).appendTo(".desktop-thead");
                $('.blue-strip-table').hide();
                $('.blue-strip-table:first').show();
            } else if($(window).width() < 768) {
                $( ".blue-strip-table" ).prependTo(".blue-strip");
                $('.blue-strip-table').hide();
                $('.blue-strip-table:first').show();
            }   
       }, 1500);
    });
       

    });
    
                
    
                var path_tick = "http://192.168.1.9/optifast-me_com/sites/all/themes/optifast/assets//images/common/right.png";
                var tick_mark = "<img src='"+path_tick+"'>";
                
                var path_cross = "http://192.168.1.9/optifast-me_com/sites/all/themes/optifast/assets//images/common/dash.png";
                var cross_mark = "<img src='"+path_cross+"'>";
    
                /*check yes or no - replacing*/
                $('a, h1, h2, h3, h4, h5, p, span, .heading, div, li').html(function(index,html){
                    return html.replace(/#yes/g , tick_mark);
                });
               $('a, h1, h2, h3, h4, h5, p, span, .heading, div, li').html(function(index,html){
                    return html.replace(/#no/g , cross_mark);
               });


    
</script>

	
