            $('#email').focus();
            focusSet = true;
        } else {
            
            	$(".success-popup").show();
			}*/
			
			if (!$('#email').val()) { 
				$(".validation").show();
									$(".validation").text("Please enter your email!");
					
				e.preventDefault();
			} else if( !pattern.test($('#email').val()) ) {
				$(".validation").show();
								$(".validation").text("Please enter a valid email address!");
					
				e.preventDefault();
			} 
			else {
				$(".form-area").hide();
				$(".success-message").show();
				//return false;
				//e.preventDefault();
			}

        
    });
		
